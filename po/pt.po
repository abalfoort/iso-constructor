#: po/en/help.html%2Bhtml.body.div.ol.li:21-13
msgid "Dist-upgrade distributions"
msgstr "Dist-upgrade ás distribuições"

#: po/en/help.html%2Bhtml.body.div.ol.li:22-13
msgid "Build ISOs"
msgstr "A construir os ISOs"

#: po/en/help.html%2Bhtml.body.div.ol.li:23-13
